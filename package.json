{
  "name": "blog-platform-api",
  "version": "0.0.1",
  "description": "A social media platfrom API.",
  "author": "vsayfb",
  "private": false,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "migrate": "concurrently \" npm run migration:generate && npm run migration:run\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "pm2 start dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:unit": "jest --selectProjects unit --watchAll",
    "test:int": "jest --selectProjects int --watchAll",
    "test:e2e": "jest --selectProjects e2e --watchAll",
    "test:watch": "jest --watchAll",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d ormconfig.ts",
    "migration:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:create ./dist/src/migrations/new",
    "migration:generate": "npm run typeorm -- migration:generate ./src/migrations/new",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert"
  },
  "dependencies": {
    "@casl/ability": "^6.5.0",
    "@elastic/elasticsearch": "^8.9.0",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/cli": "^10.1.11",
    "@nestjs/common": "^10.1.3",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.1.3",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/mapped-types": "^2.0.2",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.1.3",
    "@nestjs/platform-socket.io": "^10.1.3",
    "@nestjs/schedule": "^3.0.2",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^7.1.7",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/websockets": "^10.1.3",
    "amqplib": "^0.10.3",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cloudinary": "^1.40.0",
    "concurrently": "^8.2.0",
    "datauri": "^4.1.0",
    "form-data": "^4.0.0",
    "mailgun.js": "^9.2.0",
    "nanoid": "^3.0.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.2",
    "redis": "^4.6.7",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "short-unique-id": "^4.4.4",
    "slugify": "^1.6.6",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "twilio": "^4.14.1",
    "typeorm": "^0.3.17",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.1.3",
    "@types/amqplib": "^0.10.1",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.4.9",
    "@types/passport-jwt": "^3.0.9",
    "@types/passport-local": "^1.0.35",
    "@types/pg": "^8.10.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "29.6.2",
    "prettier": "^3.0.1",
    "socket.io-client": "^4.7.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4"
  }
}
